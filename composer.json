{
  "name": "lamphp/framework",
  "description": "The LAMPhp Framework.",
  "keywords": [
    "php",
    "framework",
    "fast",
    "easy"
  ],
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "JinZhiSu",
      "email": "happy@hacking.icu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lamp\\": "src/Lamp/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lamp\\tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "symfony/yaml": "5.4.x-dev",
    "monolog/monolog": "2.x-dev",
    "joshcam/mysqli-database-class": "dev-master",
    "guzzlehttp/guzzle": "7.4.x-dev",
    "mongodb/mongodb": "v1.8.x-dev",
    "ext-mongodb": "*",
    "doctrine/cache": "2.0.x-dev",
    "ext-mbstring": "*",
    "symfony/string": "5.4.x-dev"
  }
}
